{
  "expo": {
    "name": "ekleel_abha",
    "slug": "ekleel_abha",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/aka.png",
    "scheme": "ekleelabha",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/aka_g.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.ekleel_abha",
      "runtimeVersion": "1.0.0"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "5cce5b06-4918-46e4-91d9-36aea95c69eb"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/5cce5b06-4918-46e4-91d9-36aea95c69eb"
    }
  }
}
