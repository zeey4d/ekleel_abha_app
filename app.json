{
  "expo": {
    "name": "إكليل أبها",
    "slug": "ekleel_abha",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/ios-light.png",
    "scheme": "ekleelabha",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/icons/splash-icon-light.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
      "dark": {
        "image": "./assets/icons/splash-icon-dark.png",
        "backgroundColor": "#000000"
      }
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.zeey4d.ekleelabha",
      "icon": "./assets/icons/ios-light.png",
      "runtimeVersion": "1.0.0",
      "infoPlist": {
        "ExpoLocalization_supportsRTL": true,
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.zeey4d.ekleelabha",
      "versionCode": 1,
      "edgeToEdgeEnabled": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "runtimeVersion": "1.0.0"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/icons/ios-light.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-localization",
        {
          "supportsRTL": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "5cce5b06-4918-46e4-91d9-36aea95c69eb"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/5cce5b06-4918-46e4-91d9-36aea95c69eb"
    },
    "assetBundlePatterns": [
      "**/*"
    ]
  }
}
